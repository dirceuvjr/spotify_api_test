<h1>New spotify_artist</h1>

<% form_for(@spotify_artist) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :spotify_id %><br />
    <%= f.text_field :spotify_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :popularity %><br />
    <%= f.text_field :popularity %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', spotify_artists_path %>